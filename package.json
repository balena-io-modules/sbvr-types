    "prettify": "balena-lint -e js -e ts --fix src test"
  },
  "repository": "https://github.com/balena-io-modules/sbvr-types.git",
  "author": "",
  "license": "BSD",
  "optionalDependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "sha.js": "^2.4.11"
  },
  "devDependencies": {
    "@balena/abstract-sql-compiler": "^8.3.12",
    "@balena/lint": "^6.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/chai-datetime": "^0.0.37",
    "@types/mocha": "^10.0.0",
    "@types/sha.js": "^2.4.0",
    "chai": "^4.3.7",
    "chai-datetime": "^1.8.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "mocha": "^10.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ],
    "*.js": [
      "balena-lint --fix"
    ]
  },
  "mocha": {
    "reporter": "spec",
